import java.util.*;


public class Main
{
    static boolean isDuplicate(int arr[]){
        Set<Integer> set = new HashSet<>();
        
        for(int num : arr){
            
            if(set.contains(num)){
                return true;
            }
            else{
            set.add(num);
        }
        }
        return false;
    }
	public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
    int arr[] = new int[n];
    
    for(int i = 0; i<n; i++){
        arr[i] = sc.nextInt();
    }
    
    System.out.println(isDuplicate(arr));
	}
}
